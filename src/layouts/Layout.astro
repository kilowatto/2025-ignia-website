---
import BaseLayout from './BaseLayout.astro';

interface Props {
  title: string;
  Content_Description: string;
  canonicalUrl?: string;
  ogImage?: string;
}

const { title, Content_Description, canonicalUrl, ogImage } = Astro.props as Props;
---

<BaseLayout
  title={title}
  description={Content_Description}
  canonicalUrl={canonicalUrl}
  ogImage={ogImage}
>
  <slot />
</BaseLayout>
